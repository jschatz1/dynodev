{
  "name": "cli",
  "version": "0.0.1",
  "description": "Create your backend automatically",
  "main": "src/index.js",
  "author": "Jacob Schatz",
  "scripts": {
    "dev": "NODE_ENV=development node ./src/index.js",
    "package": "yarn rollup -c rollup.config.js",
    "build": "yarn package && yarn ncc build ./dist/bundle.js  --no-cache -o dist/almond && rm dist/bundle.js"
  },
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "camelcase-keys": "^6.2.2",
    "chalk": "^4.0.0",
    "decamelize-keys": "^1.1.0",
    "follow-redirects": "^1.11.0",
    "hard-rejection": "^2.1.0",
    "lowdb": "^1.0.0",
    "minimist-options": "^4.0.2",
    "normalize-package-data": "^2.5.0",
    "prompt": "^1.0.0",
    "redent": "^3.0.0",
    "trim-newlines": "^3.0.0",
    "yn": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-auto-install": "^2.1.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@zeit/ncc": "^0.22.1",
    "minimist": "^1.2.5",
    "nexe": "^3.3.2",
    "rollup": "^2.8.2",
    "rollup-plugin-preserve-shebangs": "^0.2.0"
  },
  "bin": {
    "almond": "./src/index.js"
  }
}
